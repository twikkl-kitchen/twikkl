externalPort = 80

[[ports]]
localPort = 5000
externalPort = 5000

[[ports]]
localPort = 5001

[[ports]]
localPort = 34625
externalPort = 3000

[[ports]]
localPort = 38523

[[ports]]
localPort = 41833

[[ports]]
localPort = 46149

[deployment]
deploymentTarget = "vm"
run = ["node", "backend/src/server.js"]
